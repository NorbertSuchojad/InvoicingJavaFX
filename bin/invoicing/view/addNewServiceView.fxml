<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="invoicing.controller.AddNewServiceController">
   <children>
      <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="40.0" prefWidth="200.0" text="WPROWADŹ DANE USŁUGI">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="80.0" prefHeight="18.0" prefWidth="175.0" text="Nazwa usługi">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="130.0" prefHeight="18.0" prefWidth="175.0" text="Domyślna jednostka">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="160.0" prefHeight="18.0" prefWidth="175.0" text="Domyślna ilość">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboService" layoutX="210.0" layoutY="15.0" prefHeight="20.0" prefWidth="370.0" promptText="Wybierz" />
      <TextField fx:id="tfServiceName" alignment="TOP_LEFT" layoutX="225.0" layoutY="80.0" prefHeight="45.0" prefWidth="325.0" promptText="Wprowadz nazwę usługi" />
      <TextField fx:id="tfSalesTyp" layoutX="225.0" layoutY="130.0" prefHeight="20.0" prefWidth="325.0" promptText="Wprowadź domyślną jednostkę sprzedaży" />
      <TextField fx:id="tfSalesValue" layoutX="225.0" layoutY="160.0" prefHeight="20.0" prefWidth="325.0" promptText="Wprowadź domyślną ilość" />
      <TextField fx:id="tfSalesPrice" layoutX="225.0" layoutY="190.0" prefHeight="20.0" prefWidth="325.0" promptText="Wprowadź domyślną cenę" />
      <Button fx:id="btnSaveAndClose" layoutX="430.0" layoutY="260.0" mnemonicParsing="false" onMouseClicked="#saveAndClose" prefHeight="20.0" prefWidth="150.0" text="Zapisz i zamknij" />
      <Button fx:id="btnSave" layoutX="270.0" layoutY="260.0" mnemonicParsing="false" onMouseClicked="#save" prefHeight="20.0" prefWidth="150.0" text="Zapisz" />
      <Button fx:id="btnSave1" layoutX="110.0" layoutY="260.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="20.0" prefWidth="150.0" text="Powrót" />
      <Label layoutX="50.0" layoutY="190.0" prefHeight="18.0" prefWidth="175.0" text="Domyślna cena">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
