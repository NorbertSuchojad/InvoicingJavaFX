            <TableColumn fx:id="col_mail" prefWidth="95.0" text="Mail" />
            <TableColumn fx:id="col_phone" prefWidth="91.0" text="Phone" />
            <TableColumn prefWidth="75.0" text="Programming">
               <columns>
                  <TableColumn fx:id="col_java" prefWidth="48.0" text="Java" />
                  <TableColumn fx:id="col_python" prefWidth="65.0" text="Python" />
                  <TableColumn fx:id="col_other" prefWidth="87.0" text="Other" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="col_language" prefWidth="97.0" text="Language" />
            <TableColumn fx:id="col_course" prefWidth="84.0" text="Course" />
        </columns>
